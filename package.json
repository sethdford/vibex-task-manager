{
	"name": "vibex-task-manager",
	"version": "0.17.26",
	"description": "A task management system for ambitious AI-driven development using AWS Bedrock exclusively.",
	"main": "index.ts",
	"type": "module",
	"bin": {
		"vibex-task-manager": "bin/vibex-task-manager.js",
		"vibex-task-manager-mcp": "bin/vibex-mcp-server.js",
		"vibex": "bin/vibex-mcp-server.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -p tsconfig.build.json --watch",
		"clean": "rm -rf dist coverage",
		"prebuild": "npm run clean",
		"postbuild": "cp scripts/modules/supported-models.json dist/scripts/modules/",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
		"test:fails": "NODE_OPTIONS=--experimental-vm-modules jest --onlyFailures",
		"test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
		"test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
		"test:e2e": "./tests/e2e/run_e2e.sh",
		"test:e2e-report": "./tests/e2e/run_e2e.sh --analyze-log",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --ext .ts,.js",
		"lint:fix": "eslint . --ext .ts,.js --fix",
		"prepare": "chmod +x bin/vibex-task-manager.js bin/vibex-mcp-server.js",
		"changeset": "changeset",
		"release": "changeset publish",
		"inspector": "npx @modelcontextprotocol/inspector node mcp-server/server.js",
		"mcp-server": "node mcp-server/server.js",
		"format-check": "biome format .",
		"format": "biome format . --write"
	},
	"keywords": [
		"claude",
		"task",
		"management",
		"ai",
		"development",
		"cursor",
		"anthropic",
		"llm",
		"mcp",
		"context"
	],
	"author": "Vibex Task Manager Contributors",
	"license": "MIT",
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "^2.2.10",
		"@aws-sdk/client-bedrock": "^3.828.0",
		"@aws-sdk/client-bedrock-runtime": "^3.828.0",
		"@aws-sdk/credential-providers": "^3.817.0",
		"ai": "^4.3.10",
		"boxen": "^8.0.1",
		"chalk": "^5.4.1",
		"cli-table3": "^0.6.5",
		"commander": "^12.0.0",
		"conf": "^12.0.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.21.2",
		"fastmcp": "^2.2.2",
		"figlet": "^1.7.0",
		"fuse.js": "^7.1.0",
		"gradient-string": "^3.0.0",
		"helmet": "^8.1.0",
		"inquirer": "^9.2.20",
		"jsonwebtoken": "^9.0.2",
		"lru-cache": "^10.2.0",
		"ora": "^8.0.1",
		"uuid": "^11.1.0",
		"zod": "^3.22.4"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sethdford/vibex-task-manager.git"
	},
	"homepage": "https://github.com/sethdford/vibex-task-manager#readme",
	"bugs": {
		"url": "https://github.com/sethdford/vibex-task-manager/issues"
	},
	"files": [
		"dist/**",
		"scripts/**",
		"assets/**",
		".cursor/**",
		"README.md",
		"index.js",
		"bin/**",
		"mcp-server/**",
		"src/**",
		"!**/*.ts",
		"!**/tsconfig*.json"
	],
	"overrides": {
		"node-fetch": "^2.6.12",
		"whatwg-url": "^11.0.0",
		"uri-templates": "npm:uri-templates-lite@^1.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.28.1",
		"@swc/core": "^1.12.1",
		"@swc/jest": "^0.2.38",
		"@types/chalk": "^0.4.31",
		"@types/figlet": "^1.5.8",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^8.34.0",
		"@typescript-eslint/parser": "^8.34.0",
		"execa": "^8.0.1",
		"ink": "^5.0.1",
		"jest": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"mock-fs": "^5.5.0",
		"prettier": "^3.5.3",
		"react": "^18.3.1",
		"supertest": "^7.1.0",
		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.16.2",
		"type-fest": "^4.41.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"aws-sdk-client-mock": "^3.0.1"
	}
}
